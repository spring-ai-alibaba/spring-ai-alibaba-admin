{
  "name": "prompt-engineering-system",
  "version": "1.0.0",
  "description": "Prompt工程管理系统",
  "scripts": {
    "build:css": "tailwindcss -i ./src/styles/index.css -o ./src/styles/tailwind.css --minify",
    "watch:css": "tailwindcss -i ./src/styles/index.css -o ./src/styles/tailwind.css --minify --watch",
    "dev": "concurrently \"pnpm watch:css\" \"rspack serve\"",
    "build": "concurrently \"pnpm build:css\" \"rspack build\""
  },
  "dependencies": {
    "@ant-design/icons": "^6.0.0",
    "@codemirror/lang-json": "^6.0.2",
    "@fortawesome/fontawesome-free": "^6.4.0",
    "@uiw/codemirror-theme-github": "^4.25.1",
    "@uiw/react-codemirror": "^4.25.1",
    "antd": "^5.27.1",
    "codemirror": "^6.0.2",
    "dayjs": "^1.11.15",
    "diff2html": "^3.4.52",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-json-view": "^1.21.3",
    "react-router-dom": "^6.8.0"
  },
  "devDependencies": {
    "@anthropic-ai/claude-code": "^1.0.119",
    "@rspack/cli": "^1.4.11",
    "@rspack/core": "^1.4.11",
    "@tailwindcss/cli": "^4.1.12",
    "@tailwindcss/postcss": "^4.1.12",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "autoprefixer": "^10.4.16",
    "code-inspector-plugin": "^1.2.10",
    "concurrently": "^9.2.0",
    "css-loader": "^6.8.1",
    "html-webpack-plugin": "^5.5.3",
    "node-polyfill-webpack-plugin": "^4.1.0",
    "postcss": "^8.4.31",
    "postcss-loader": "^7.3.3",
    "style-loader": "^3.3.3",
    "tailwindcss": "^3.3.5",
    "typescript": "^5.9.2"
  }
}
